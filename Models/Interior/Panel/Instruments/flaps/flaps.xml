<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>flaps.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>flaps</object-name>

  </animation>

  <animation>
    <type>rotate</type>
    <object-name>flaps</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.33 </ind><dep> 30 </dep></entry>
      <entry><ind> 0.66 </ind><dep> 60 </dep></entry>
      <entry><ind> 1.00 </ind><dep> 90 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
